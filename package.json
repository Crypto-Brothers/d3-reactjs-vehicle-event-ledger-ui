    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^5.2.0",
    "eslint-config-synacor": "^3.0.1",
    "if-env": "^1.0.4",
    "node-sass": "^4.9.2",
    "preact-cli": "^2.2.1",
    "sass-loader": "^7.1.0"
  },
  "dependencies": {
    "d3": "^5.5.0",
    "mitt": "^1.1.3",
    "preact": "^8.2.9",
    "preact-compat": "^3.18.2",
    "preact-router": "^2.6.1",
    "rxjs": "^6.2.2"
  }
