    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-synacor": "^2.0.2",
    "if-env": "^1.0.0",
    "node-sass": "^4.7.2",
    "preact-cli": "^2.0.1",
    "sass-loader": "^6.0.6"
  },
  "dependencies": {
    "d3": "^4.12.0",
    "mitt": "^1.1.3",
    "preact": "^8.2.6",
    "preact-compat": "^3.17.0",
    "preact-router": "^2.5.7",
    "rxjs": "^5.5.6"
  }
